{
  "name": "MySolomanAI",
  "version": "1.0.0",
  "description": "Christian AI with more Powerful than ChatGPT 3.5 and an Expert on Christianity and the Bible",
  "main": "main.js",
  "scripts": {
    "package-win": "electron-packager . MySolomonAI --platform=win32 --arch=x64 --out=dist-win --overwrite --icon=assets/favicon.ico --extra-resource=.env",
    "dev": "npx webpack --mode development && npx electron .",
    "start": "npx electron .",
    "build": "npx webpack --mode production && electron-builder"
  },
  "build": {
    "appId": "com.enocha.mysolomonai",
    "win": {
      "target": "nsis",
      "icon": "assets/favicon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/favicon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/favicon.ico"
    },
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "dist/**/*",
      "main.js",
      "index.html",
      "preload.js",
      "env"
    ],
    "extraResources": [
      {
        "from": "resources/env",
        "to": "resources/env"
      }
    ]
  },
  "keywords": [],
  "author": "Enoch Abiodun",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@langchain/community": "^0.2.12",
    "@langchain/core": "^0.2.7",
    "@langchain/mongodb": "^0.0.4",
    "@langchain/openai": "^0.1.3",
    "@mui/joy": "^5.0.0-beta.36",
    "@mui/lab": "^5.0.0-alpha.170",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "langchain": "^0.2.5",
    "moment": "^2.30.1",
    "mongodb": "^6.7.0",
    "mongoose": "^8.4.3",
    "node-localstorage": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-input-emoji": "^5.9.0",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "electron": "^31.0.1",
    "electron-builder": "^24.13.3",
    "style-loader": "^4.0.0",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
